{
    "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
    "version": 2,
    "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
        "foreground": "p:white",
        "style": "powerline",
        "template": "{{ if .WSL }}WSL at {{.Icon}}  {{ end }}",
        "type": "os"
        },
        {
        "foreground": "p:red",
        "style": "plain",
        "template": "{{ if .WSL }}root{{else}}\uf0e7{{ end }} ",
        "type": "root"
        },
        {
        "foreground": "p:blue",
        "style": "powerline",
        "template": "\ue798 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} | ",
        "type": "dart"
        },
        {
        "foreground": "p:green",
        "style": "powerline",
        "template": "\ue718 {{ if .PackageManagerIcon }}{{ .PackageManagerIcon }} {{ end }}{{ .Full }} | ",
        "type": "node"
        },
        {
        "foreground": "p:yellow",
        "properties": {
            "fetch_virtual_env": true
        },
        "style": "plain",
        "template": "\ue235 {{ .Venv }} ",
        "type": "python"
        },
        {
            "foreground": "p:foreground",
            "properties": {
                "style": "full"
            },
            "style": "plain",
            "template": "{{ .Path }} ",
            "type": "path"
        },
        {
          "foreground": "p:blue",
          "properties": {
            "branch_icon": "\ue725 ",
            "cherry_pick_icon": "\ue29b ",
            "commit_icon": "\uf417 ",
            "fetch_status": false,
            "fetch_upstream_icon": false,
            "merge_icon": "\ue727 ",
            "no_commits_icon": "\uf0c3 ",
            "rebase_icon": "\ue728 ",
            "revert_icon": "\uf0e2 ",
            "tag_icon": "\uf412 "
          },
          "style": "plain",
          "template": "{{ .HEAD }} ",
          "type": "git"
        },
        {
            "foreground": "p:yellow",
            "foreground_templates": [
              "{{ if gt .Code 0 }}p:red{{ end }}"
            ],
            "properties": {
              "always_enabled": true
            },
            "style": "plain",
            "template": "> ",
            "type": "status"
          }
      ],
      "type": "prompt"
    }
  ],
  "transient_prompt": {
    "template": "{{ .Folder }}<#ffdd86> > </>",
    "foreground": "foreground"
  },
  "palette": {
    "background": "#1D1F21",
    "black": "#000000",
    "blue": "#81A2BE",
    "brightBlack": "#000000",
    "brightBlue": "#81A2BE",
    "brightCyan": "#8ABEB7",
    "brightGreen": "#B5BD68",
    "brightPurple": "#B294BB",
    "brightRed": "#CC6666",
    "brightWhite": "#FFFFFF",
    "brightYellow": "#F0C674",
    "cursorColor": "#C5C8C6",
    "cyan": "#8ABEB7",
    "foreground": "#C5C8C6",
    "green": "#B5BD68",
    "purple": "#B294BB",
    "red": "#CC6666",
    "selectionBackground": "#373B41",
    "white": "#FFFFFF",
    "yellow": "#F0C674"
  }
}
