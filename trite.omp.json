{
    "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
    "version": 2,
    "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
        "foreground": "white",
        "style": "powerline",
        "template": "{{ if .WSL }}WSL at {{.Icon}}  {{ end }}",
        "type": "os"
        },
        {
        "foreground": "red",
        "style": "plain",
        "template": "{{ if .WSL }}root{{else}}admin{{ end }} ",
        "type": "root"
        },
        {
        "foreground": "#06A4CE",
        "style": "powerline",
        "template": "\ue798 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} | ",
        "type": "dart"
        },
        {
        "foreground": "#6CA35E",
        "style": "powerline",
        "template": "\ue718 {{ if .PackageManagerIcon }}{{ .PackageManagerIcon }} {{ end }}{{ .Full }} | ",
        "type": "node"
        },
        {
        "foreground": "#FFDE57",
        "properties": {
            "fetch_virtual_env": true
        },
        "style": "plain",
        "template": "\ue235 {{ .Venv }} ",
        "type": "python"
        },
        {
            "foreground": "white",
            "properties": {
                "style": "full"
            },
            "style": "plain",
            "template": "{{ .Path }} ",
            "type": "path"
        },
        {
          "foreground": "cyan",
          "properties": {
            "branch_icon": "\ue725 ",
            "cherry_pick_icon": "\ue29b ",
            "commit_icon": "\uf417 ",
            "fetch_status": false,
            "fetch_upstream_icon": false,
            "merge_icon": "\ue727 ",
            "no_commits_icon": "\uf0c3 ",
            "rebase_icon": "\ue728 ",
            "revert_icon": "\uf0e2 ",
            "tag_icon": "\uf412 "
          },
          "style": "plain",
          "template": "{{ .HEAD }} ",
          "type": "git"
        },
        {
            "foreground": "#ffdd86",
            "foreground_templates": [
              "{{ if gt .Code 0 }}#42a9ff{{ end }}"
            ],
            "properties": {
              "always_enabled": true
            },
            "style": "plain",
            "template": "> ",
            "type": "status"
          }
      ],
      "type": "prompt"
    }
  ],
  "transient_prompt": {
    "template": "{{ .Folder }} ",
    "foreground": "white"
  }
}
